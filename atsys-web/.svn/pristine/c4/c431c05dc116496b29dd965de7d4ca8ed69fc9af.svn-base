html {
    height: 100%;
    font-family: san-serif, tahoma, georgia, serif !important;
    font-size: 0.8em;
}

hr {
    opacity: 0.4;
}

body {
    margin: 0;
    padding: 0;
    min-height: 100%;
    text-rendering: optimizeSpeed;
    color: #1d1d1d;
    /*box-shadow: inset 0 0 30px rgba(0, 0, 0, 0.6);*/
}

label {
    font-weight: bold;
}

video {
    display: block;
}

a.title span, .title {
    color: #fff;
    text-shadow: 0 0 10px #fff, 0 0 20px #ff2d95, 0 0 25px #ff2d95, 0 0 30px #ff2d95, 0 0 40px #ff2d95, 0 0 50px #ff2d95, 0 0 80px #ff2d95;
    letter-spacing: 5px;
}

h2, h3 {
    text-shadow: rgba(255, 255, 255, 0.4) 0 1px 0;
}

h1, h2, h3 {
    margin: 3px 0;
    padding: 3px 0;
}

.m-right {
    margin-right: 10px !important;
}

.m-top {
    margin-top: 10px;
}

.sw {
    /* Sombra preta.*/
    box-shadow: 0 5px 10px rgba(0, 0, 0, 0.8) !important;
    /* Sombra branca. */
    /*box-shadow: 0 5px 10px rgba(255, 255, 255, 0.7) !important;*/
}

.title-sh {
    color: #161616;
    text-shadow: rgba(255, 255, 255, 0.4) 0 1px 0;
}

.trans {
    background: rgba(0, 0, 0, 0.2);
}

.ui-state-disabled {
    opacity: 0.7 !important;
}

::selection {
    /*background: #D14;*/
    background: #96238F;
    color: white;
}

::-moz-selection {
    /*background: #D14; */
    background: #96238F;
    color: white;
}

/* Para o overlay do dialog. */
.ui-widget-overlay {
    background: rgba(0, 0, 0, 0.8) !important;
}

/* Para corrigir as bordas da tabela no Firefox. */
td {
    border-color: #d0e4de !important;
}

/* ============================ */
/* Formatação para formulários. */
/* ============================ */

label {
    font-size: 0.9em !important;
}

div.form, div.form > div {
    position: relative;
}

div.form > label > *, div.form > div > label > * {
    display: block;
    margin-top: 2px;
}

/* Importante para não aparecer os elementos 'script'. */
script {
    display: none !important;
}

div.form > *, div.form > div > * {
    margin: 5px 10px 5px 0;
    display: inline-block;
    vertical-align: bottom !important;
}

/* ============================ */
/* Componente Growl. */
/* ============================ */

.ui-growl-item {
    background-color: #313e93 !important;
    -webkit-border-radius: 3px !important;
    -moz-border-radius: 3px !important;
    color: #fff !important;
    border: solid white 2px !important;
}

tr.ui-expanded-row-content > td {
    background-color: #D4D7DF;
    box-shadow: inset 0 0 20px rgba(0, 0, 0, 0.5);
}

/* ============================ */
/* Formatação especial para o autocomplete. */
/* ============================ */

table.ui-autocomplete-items {
    overflow: hidden !important;
}

/* ============================ */
/* Formatação especial para o menu. */
/* ============================ */

#menu-master.menu {
    /* Fixa o menu na tela. */
    position: fixed !important;
    z-index: 1000;
    width: 100%;
    border: none;
}

/* Corrige o problema do submenu que desaparece no Firefox. */
#menu-master.ui-menu-child {
    z-index: 9000;
    top: 30px !important;
}

#menu-master .ui-menu, #menu-master .ui-menu-child {
    /* Para corrigir alguns temas.*/
    float: none !important;
    background: -webkit-gradient(linear, left top, left bottom, from(#343434), to(#1d1d1d));
    background: -moz-linear-gradient(top, #343434, #1d1d1d);
    /*-webkit-box-shadow: 0px 5px 20px 0px rgba(0, 0, 0, 0.8);*/
    /*-moz-box-shadow: 0px 5px 20px 0px rgba(0, 0, 0, 0.8);*/
    box-shadow: 0 2px 3px 3px rgba(0, 0, 0, 0.3);
}

#menu-master .ui-menu-list, .ui-menuitem, #menu-master .ui-menuitem-hover {
    /*background: inherit;*/
    border: none;
}

#menu-master li.ui-menuitem-active a.ui-state-hover {
    background: transparent;
}

/* Efeito nos textos do menu. */
#menu-master li.ui-menuitem-active a.ui-state-hover span {
    background: transparent;
    color: #ffffff !important;
    text-shadow: 0 0 20px #ffffff, 2px 2px 10px #ffffff;
}

#menu-master .ui-menuitem-text, #menu-master a.ui-state-disabled span {
    background: transparent;
    color: #d6d6db;
    /*text-shadow: 0 0 2px #d6d6db;*/
}

/* Remove as bordas redondas. */
#menu-master div[role="menubar"] {
    -moz-border-radius: 0px;
    -webkit-border-radius: 0px;
    border-radius: 0px;
    border: none !important;
}

.ui-state-focus {
    -moz-box-shadow: 0 0 20px darkorange !important;
    -webkit-box-shadow: 0 0 20px darkorange !important;
    box-shadow: 0 0 20px darkorange !important;
}

.main {
    min-height: 100%;
    padding-top: 25px
}

.clr {
